{
  "configurations": {
    "launch - Compile Example": {
      "adapter": "netcoredbg",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/Lang/bin/Debug/net5.0/Lang.dll",
        "args": ["-S", "${workspaceRoot}/Example/main.ol"],
        //"args": ["-S", "${workspaceRoot}/Tests/tests/basic/basic.ol"],
        "stopAtEntry": false,
        "cwd": "${workspaceRoot}"
      }
    },
    "Launch - Debug Example": {
      "adapter": "vscode-cpptools",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/Example/bin/Example",
        "cwd": "${workspaceRoot}",
        "externalConsole": true,
        "MIMode": "gdb"
      }
    }
  }
}

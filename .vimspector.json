{
  "configurations": {
    "launch - Compile Example": {
      "adapter": "netcoredbg",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/ol/bin/Debug/net6.0/ol.dll",
        "args": ["-S", "${workspaceRoot}/example/main.ol", "-noThreads"],
        //"args": ["-S", "${workspaceRoot}/test/tests/x11/x11.ol", "-noThreads"],
        //"args": ["-S", "${workspaceRoot}/hp/hp.ol", "-noThreads"],
        "stopAtEntry": false,
        "cwd": "${workspaceRoot}"
      }
    },
    "Launch - Debug Example": {
      "adapter": "vscode-cpptools",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/example/bin/Example",
        "cwd": "${workspaceRoot}",
        "externalConsole": true,
        "MIMode": "gdb"
      }
    }
  }
}
